059E   5D                     LD   E,L   ;        LD DE,HL
059F   54                     LD   D,H   ; 
05A0   29                     ADD   HL,HL   ;        4X
05A1   29                     ADD   HL,HL   ;        8X
05A2   19                     ADD   HL,DE   ;        2X  + 8X  = 10X
05A3                             ; 52t cycles
05A3   18 E6                  JR   num1   
